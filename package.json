{
  "name": "ui5con24-dev-env",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "pnpm --filter @capire/bookshop watch",
    "start:server:dev": "pnpm --filter @capire/bookshop watch:dev",
    "start:server:setup": "pnpm --filter @capire/bookshop setup",
    "start:app": "pnpm --filter ui5con24.app start",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "start:app:silent": "pnpm --filter ui5con24.app start:silent",
    "start:app-abap": "pnpm --filter ui5con24.app.abap start",
    "start:app-abap:silent": "pnpm --filter ui5con24.app.abap start:silent"
  },
