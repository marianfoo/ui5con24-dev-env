{
	"name": "ui5con24.app.abap",
	"version": "1.0.0",
	"description": "UI5 Application: ui5con24.app.abap",
	"author": "„marianfoo“",
	"license": "Apache-2.0",
	"scripts": {
		"clean": "rimraf dist coverage",
		"build": "ui5 build --config ui5-build.yaml --include-dependency moment --clean-dest",
		"build:opt": "ui5 build self-contained --clean-dest --all",
		"start": "ui5 serve --port 8081 -o index.html",
		"start:silent": "ui5 serve --port 8081",
		"start-cdn": "ui5 serve --port 8081 -o index-cdn.html",
		"start:dist": "npm start -- --config ui5-dist.yaml",
		"start:dist-cdn": "npm run start-cdn -- --config ui5-dist.yaml",
		"ts-typecheck": "tsc --noEmit",
		"lint": "eslint webapp",
		"karma": "karma start",
		"karma-ci": "karma start karma-ci.conf.js",
		"karma-ci-cov": "karma start karma-ci-cov.conf.js",
		"test": "npm run lint && npm run karma-ci-cov",
		"deploy:abap": "npm run build && fiori deploy --config ui5-deploy.yaml -y",
		"deploy:only": "fiori deploy --config ui5-deploy.yaml -y"
	},
	"dependencies": {
		"moment": "2.30.1",
		"ui5-cc-helloworld": "0.0.2",
		"ui5-cc-control": "workspace:*",
		"ui5con24.component": "workspace:*",
		"ui5con24.lib.abap": "workspace:*"
	},
	"devDependencies": {
		"@openui5/types": "^1.124.0",
		"@sap/ux-ui5-tooling": "^1.13.5",
		"@sapui5/types": "1.124.0",
		"@typescript-eslint/eslint-plugin": "^7.11.0",
		"@typescript-eslint/parser": "^7.11.0",
		"@ui5/cli": "^3.10.3",
		"eslint": "^8.56.0",
		"karma": "^6.4.3",
		"karma-chrome-launcher": "^3.2.0",
		"karma-coverage": "^2.2.1",
		"karma-ui5": "^3.0.4",
		"karma-ui5-transpile": "^3.4.2",
		"rimraf": "^5.0.7",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.5",
		"ui5-middleware-livereload": "^3.0.3",
		"ui5-middleware-serveframework": "3.1.1",
		"ui5-middleware-simpleproxy": "3.2.16",
		"ui5-middleware-ui5": "3.1.5",
		"ui5-tooling-modules": "3.6.0",
		"ui5-tooling-transpile": "^3.4.2"
	}
}
