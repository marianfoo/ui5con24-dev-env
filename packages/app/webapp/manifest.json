{
	"_version": "1.12.0",

	"sap.app": {
		"id": "ui5con24.app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "${version}"
		},
		"crossNavigation": {
		  "inbounds": {
			"intent1": {
			  "signature": {
				"parameters": {},
				"additionalParameters": "allowed"
			  },
			  "semanticObject": "ui5con24",
			  "action": "120",
			  "title": "UI5con 24",
			  "info": "",
			  "subTitle": "UI5 1.120",
			  "icon": "sap-icon://course-book"
			}
		  }
		},
		"dataSources": {
		  "mainService": {
			"uri": "/admin/",
			"type": "OData",
			"settings": {
			  "annotations": [],
			  "odataVersion": "4.0"
			}
		  }
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"fullWidth": true
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "ui5con24.app.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},

		"dependencies": {
			"minUI5Version": "1.120.13",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"ui5con24.lib": {}
			},
			"components":{
				"ui5con24.component": {}
			}
		},

		"componentUsages": {
			"ui5con24component": {
			  "name": "ui5con24.component"
			},
			"spreadsheetImporter": {
				"name": "cc.spreadsheetimporter.v0_34_1"
			}
		  },
		  "resourceRoots": {
			"ui5con24.component": "./ui5con24/component/"
		  },

		"handleValidation": true,

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"models": {
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
				  "synchronizationMode": "None",
				  "operationMode": "Server",
				  "autoExpandSelect": true,
				  "earlyRequests": true
				}
			  },
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ui5con24.app.i18n.i18n"
				}
			}
		},

		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "ui5con24.app.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "main",
					"target": "main"
				}
			],
			"targets": {
				"main": {
					"viewId": "main",
					"viewName": "Main"
				}
			}
		}
	}
}
