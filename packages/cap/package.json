{
  "name": "@capire/bookshop",
  "version": "1.0.0",
  "description": "A simple self-contained bookshop service.",
  "private": true,
  "devDependencies": {
    "cds-launchpad-plugin": "2.1.8",
    "cds-plugin-ui5": "^0.8.3",
    "@cap-js/sqlite": "^1.7.0"
  },
  "dependencies": {
    "@sap/cds": "^7.9.0",
    "@sap/cds-dk": "^7.9.2",
    "@sap/cds-hana": "^2.0.0",
    "@sap/xsenv": "^5.1.0",
    "@sap/xssec": "3.6.1",
    "express": "^4.19.2",
    "passport": "^0.7.0"
  },
  "scripts": {
    "genres": "cds serve test/genres.cds",
    "start": "cds-serve",
    "watch": "cds watch",
    "build": "cds build --production"
  },
  "cds": {
    "requires": {
      "[production]": {
        "auth": "xsuaa",
        "db": {
          "kind": "hana"
        }
      },
      "db": {
        "kind": "sqlite",
        "url": "db.sqlite"
      },
      "auth": "dummy"
    },
    "cds-plugin-ui5": {
      "modules": {
        "ui5con24.app": {
          "configFile": "ui5.yaml"
        }
      }
    },
    "launchpad": {
      "theme": "sap_horizon",
      "version": "1.120.14",
      "modulePaths": {
        "ui5con24.lib": "./ui5con24.app/resources/ui5con24/lib",
        "test": "./adsfasf"
    }
    }
  },
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  }
}
