  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to...",
      "type": "node",
      "request": "attach",
      "processId": "${command:PickProcess}",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "cds run",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npx",
      "runtimeArgs": ["-n"],
      "args": ["--", "cds", "run", "--with-mocks", "--in-memory?"],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
